local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Project Ultima", "Serpent")

local Tab = Window:NewTab("Main")

local Section = Tab:NewSection("Stuff")

Section:NewButton("Instant Win", "Instant Kill Your Enemy Npc", function()
    for i,v in pairs(game.Players.LocalPlayer.OppPokemon:GetChildren()) do
        v.CurrentHP.Value = 0
        v.Status.Value = "Faint"
    end
end)


Section:NewButton("Save", "Saves The Game", function()
game:GetService("ReplicatedStorage").Eventz.Internalsa123.SaveD:InvokeServer()

end)

Section:NewButton("Heal Pokemons", "Heal Your Pokemons", function()
for i,v in pairs(game.Players.LocalPlayer.PokemonParty:GetChildren()) do
        v.CurrentHP.Value = v.Stats.HPStat.Value
        v.Status.Value = ""
    end
    if game.Players.LocalPlayer:FindFirstChild('PokemonParty2') then
        for i,v in pairs(game.Players.LocalPlayer:FindFirstChild('PokemonParty2'):GetChildren()) do
            v.CurrentHP.Value = v.Stats.HPStat.Value
            v.Status.Value = ""
        end
    end
end)

Section:NewButton("Catch Faster", "Sleeps & 1hp the Pokemon you tries to catch.", function()
for i,v in pairs(game.Players.LocalPlayer.OppPokemon:GetChildren()) do
        v.CurrentHP.Value = 1
        v.DateMet.Value = "Long time ago..."
        v.Status.Value = "Sleep"
    end 
end)

local Section = Tab:NewSection("Stuff for now")

Section:NewButton("Get some Master balls","Gives 100 Master balls", function()
    local args = {
        [1] = "Master Ball",
        [2] = game:GetService("Players").LocalPlayer.Bag.Pokeball,
        [3] = 100,
        [4] = "tetst"
    }
    
    game:GetService("ReplicatedStorage").Eventz.Pokemonaaa123.adjgsskaepokzzz:InvokeServer(unpack(args))
end)

Section:NewButton("Get some Blue Orbs","Gives 100 Red Orbs", function()
    local args = {
        [1] = "Blue Orb",
        [2] = game:GetService("Players").LocalPlayer.Bag.HeldItems,
        [3] = 100,
        [4] = "tetst"
    }
    
