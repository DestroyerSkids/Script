local Tab = Window:NewTab("Money")

local Money = Tab:NewSection("Here get some money!")

Money:NewToggle("Ez Money", "Gives alot of money!", function(value) --the (arg) is if the checkbox is toggled or not
    if value then
        _G.Basic = true 
        while wait(0) and _G.Basic do
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2700.4450683594, -78.731864929199, -965.42535400391)
local args = {
    [1] = "Abraxas"
}

game:GetService("ReplicatedStorage").Eventz.Pokemonaaa123.Entity:InvokeServer(unpack(args))

        end
    else
        _G.Basic = false
    end
end)


local Tab = Window:NewTab("Misc")

local Tabs = Tab:NewSection("Misc Stuff")

Tabs:NewKeybind("Keybind", "Keybind for the Gui/Hub", Enum.KeyCode.RightShift, function()
	Library:ToggleUI()
end)

Tabs:NewButton("Infinite Yield", "Executes Infinite Yield", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

Tabs:NewButton("Flight", "Click E for use Flight", function()
loadstring(game:HttpGet("https://pastebin.com/raw/0gE7aLS1"))()
end)

Tabs:NewButton("AntiLag/BoostFps", "Boost your fps.", function()
    workspace:FindFirstChildOfClass('Terrain').WaterWaveSize = 0
	workspace:FindFirstChildOfClass('Terrain').WaterWaveSpeed = 0
	workspace:FindFirstChildOfClass('Terrain').WaterReflectance = 0
	workspace:FindFirstChildOfClass('Terrain').WaterTransparency = 0
	game:GetService("Lighting").GlobalShadows = false
	game:GetService("Lighting").FogEnd = 9e9
	settings().Rendering.QualityLevel = 1
	for i,v in pairs(game:GetDescendants()) do
		if v:IsA("Part") or v:IsA("UnionOperation") or v:IsA("MeshPart") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
			v.Material = "Plastic"
			v.Reflectance = 0
		elseif v:IsA("Decal") then
			v.Transparency = 1
		elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
			v.Lifetime = NumberRange.new(0)
		elseif v:IsA("Explosion") then
			v.BlastPressure = 1
			v.BlastRadius = 1
		end
	end
	for i,v in pairs(game:GetService("Lighting"):GetDescendants()) do
		if v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("ColorCorrectionEffect") or v:IsA("BloomEffect") or v:IsA("DepthOfFieldEffect") then
			v.Enabled = false
		end
	end
    end)

Tabs:NewSlider("WalkSpeed", "Changes your Walking speed", 120, 16, function(s) -- 500 (MaxValue) | 16 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

Tabs:NewButton("Fullbright","Makes it fullbright!", function()
    if brightLoop then
		brightLoop:Disconnect()
	end
	local function brightFunc()
		game:GetService("Lighting").Brightness = 2
		game:GetService("Lighting").ClockTime = 14
		game:GetService("Lighting").FogEnd = 100000
		game:GetService("Lighting").GlobalShadows = false
		game:GetService("Lighting").OutdoorAmbient = Color3.fromRGB(128, 128, 128)
	end

	brightLoop = game:GetService("RunService").RenderStepped:Connect(brightFunc)
end)

Tabs:NewLabel("Made by ScriptHaxer")
