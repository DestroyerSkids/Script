        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-3464.7341308594, -58.600807189941, 386.09100341797)
    elseif teleportPlace == "Lavender Town" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2902.474609375, -57.726642608643, 193.75198364258)
    elseif teleportPlace == "Celadon" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-2671.5891113281, -79.165634155273, -1435.9185791016)
    elseif teleportPlace == "Fuchsia City" then
