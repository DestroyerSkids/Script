function teleportTo(teleportPlace)
    if teleportPlace == "Home" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(1, 1, 1)
    elseif teleportPlace == "Virdian" then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(864.95391845703, -132.07646179199, 1123.5589599609)
    elseif teleportPlace == "Pewter" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(860.2119140625, -113.95027923584, 2015.0466308594)
    elseif teleportPlace == "Cerulean" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-448.78854370117, -103.57664489746, 1574.6273193359)
